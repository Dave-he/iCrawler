        "selector": "h1",
        "timeout": 5000
      }
    },
    {
      "id": "3",
      "type": "browserAction",
      "position": { "x": 700, "y": 100 },
      "data": {
        "label": "Get Page Title",
        "action": "executeScript",
        "script": "return document.title;"
      }
    },
    {
      "id": "4",
      "type": "dataMapper",
      "position": { "x": 1000, "y": 100 },
      "data": {
        "label": "Map Results",
        "mappingRules": {
          "pageTitle": "browserResult.result",
          "timestamp": "new Date().toISOString()"
        },
        "keepOriginal": true
      }
    },
    {
      "id": "5",
